{
  "buildCommand": "cd svelte && npm run build",
  "outputDirectory": "svelte/dist",
  "installCommand": "cd svelte && npm install",
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/main.py"
    }
  ]
}