{
  "version": 2,
  "builds": [
    {
      "src": "svelte/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/svelte/dist/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/main.py"
    }
  ],
  "functions": {
    "main.py": {
      "maxDuration": 30
    }
  }
}
